.tabbar{
    width: 100%;
    display: flex;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    padding-bottom: env(safe-area-inset-bottom);
    padding-top: 10rpx;
    z-index: 9999;
}

.tabbar-item{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.tabbar-item image{
    width: 50rpx;
    height: 50rpx;
}

.tabbar-item text{
    font-size: 26rpx;
    margin-top: 5rpx;
}

.tabbar-item .publish{
    background-color: #ff90b4;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: auto;
}

.tabbar-item .publish text{
    font-size: 65rpx;
    color: #fff;
}

.tabbar-select{
    color: #ff90b4;
}